# include    < iostream > 
 using   namespace   std ; 
 # define   maxn   1000 
 int   A [ maxn ] ; 
 int   n ; 
 int   main ( )    { 
   cin    > >   n ; 
   for    ( int   i    =   1 ;   i    < =   n ;    + + i )    { 
     cin    > >   A [ i ] ; 
   } 
   int   result    =   0 ; 
   for    ( int   i    =   1 ;   i    < =   n ;    + + i )    { 
     for    ( int   j    =   i    +   1 ;   j    < =   n ;    + + j )    { 
       if    ( A [ i ]    >   A [ j ] )    { 
         result    + =    ( i   ^   j ) ; 
       } 
     } 
   } 
   cout    < <   result    < <   endl ; 
 } 
